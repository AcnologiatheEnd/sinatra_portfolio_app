<p>@<%= @user.username %></p>
<p>points:<%= @user.points %></p><br>

<% @user.items.each do |item| %>
    <% if item.status == "selling" %>
        <h3><%= item.name%></h3>
        <p><%= item.description%></p>
        <p><%= item.status%></p>
        <p>$<%= item.cost%></p>
        <a href="/item/<%=item.id%>/edit"><p>edit</p></a>
        <a href="/item/<%=item.id%>/delete"><p>delete</p></a><br>
    <% else %>
        <h3><%= item.name%></h3>
        <p><%= item.description%></p>
        <p><%= item.status%></p>
        <p>$<%= item.cost%></p><br>
    <% end %>
<% end %>